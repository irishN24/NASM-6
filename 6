section .text

global _start

_start:

;ввод строки
    mov     eax, 3
    mov     ebx, 0
    mov     ecx, strA
    mov     edx, 255
    int     0x80

    mov     eax, [x]
    sub     eax, '0'
    mov     ebx, [y]
    sub     ebx, '0'
    add     eax, ebx
    add     eax, '0'

    mov     [sum], eax

    

    mov     ecx, sum
    mov     edx, 1
    mov     ebx, 1
    mov     eax, 4
    int     0x80

    mov     eax, 1
    int     0x80

section .data
   

segment .bss

    strA resb 255
